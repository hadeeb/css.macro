{
  "name": "@hadeeb/css.macro",
  "version": "0.0.1",
  "description": "A tiny CSS in JS solution for simple use cases",
  "main": "src/macro.js",
  "repository": "https://github.com/hadeeb/css.macro.git",
  "author": "Hadeeb Farhan <hadeebfarhan1@gmail.com>",
  "license": "MIT",
  "files": [
    "src",
    "runtime",
    "server.js"
  ],
  "scripts": {
    "test": "jest",
    "posttest": "size-limit"
  },
  "peerDependencies": {
    "babel-plugin-macros": ">=2"
  },
  "dependencies": {
    "@babel/helper-annotate-as-pure": "^7.8.3",
    "@babel/helper-module-imports": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "postcss": "^7.0.27",
    "postcss-csso": "^4.0.0",
    "postcss-js": "^2.0.3",
    "postcss-nested": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@size-limit/preset-small-lib": "^4.0.2",
    "@types/autoprefixer": "^9.7.0",
    "@types/babel-plugin-macros": "^2.8.0",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.21",
    "babel-plugin-macros": "^2.8.0",
    "babel-test": "^0.2.3",
    "jest": "^25.1.0",
    "react": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "size-limit": "^4.0.2"
  },
  "browser": {
    "./runtime/sheet.js": "./runtime/sheet.browser.js"
  },
  "keywords": [
    "babel-plugin-macros",
    "css-in-js",
    "styled"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "size-limit": [
    {
      "name": "css",
      "path": "runtime/css.js",
      "limit": "140 B"
    },
    {
      "name": "css + styled",
      "path": [
        "runtime/styled.js",
        "runtime/css.js"
      ],
      "limit": "210 B",
      "ignore": [
        "react"
      ]
    }
  ]
}
